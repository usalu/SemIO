{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/ConnectElementResponse",
    "definitions": {
        "ConnectElementResponse": {
            "properties": {
                "connected_element_pose": {
                    "$ref": "#/definitions/semio.model.v1.Pose",
                    "additionalProperties": true
                },
                "connection_point": {
                    "$ref": "#/definitions/semio.geometry.v1.Point",
                    "additionalProperties": true
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Connect Element Response"
        },
        "semio.geometry.v1.Point": {
            "properties": {
                "x": {
                    "type": "number",
                    "description": "X coordinate."
                },
                "y": {
                    "type": "number",
                    "description": "Y coordinate"
                },
                "z": {
                    "type": "number",
                    "description": "Z coordinate"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Point",
            "description": "A 3d point with x,y,z coordinates."
        },
        "semio.geometry.v1.Quaternion": {
            "properties": {
                "w": {
                    "type": "number",
                    "description": "The value of the real part of the quaternion."
                },
                "x": {
                    "type": "number",
                    "description": "The first value of the imaginary part of the quaternion."
                },
                "y": {
                    "type": "number",
                    "description": "The second value of the imaginary part of the quaternion."
                },
                "z": {
                    "type": "number",
                    "description": "The third value of the imaginary part of the quaternion."
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Quaternion",
            "description": "(Unit) quaternions represent (here) (rotational) orientation. It can be interpreted as the view of an object."
        },
        "semio.model.v1.Pose": {
            "properties": {
                "point_of_view": {
                    "$ref": "#/definitions/semio.geometry.v1.Point",
                    "additionalProperties": true,
                    "description": "Point of view (origin) from the pose (reference frame/coordinate system)"
                },
                "view": {
                    "$ref": "#/definitions/semio.geometry.v1.Quaternion",
                    "additionalProperties": true,
                    "description": "View (orientation) from the pose (reference frame/coordinate system)"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pose",
            "description": "A pose is a static reference frame/coordinate system."
        }
    }
}