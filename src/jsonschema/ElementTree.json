{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/ElementTree",
    "definitions": {
        "ElementTree": {
            "properties": {
                "element_id": {
                    "type": "string",
                    "description": "An optional element id"
                },
                "pose": {
                    "$ref": "#/definitions/semio.model.v1.Pose",
                    "additionalProperties": true,
                    "description": "An optional pose of the element tree."
                },
                "type": {
                    "type": "string",
                    "description": "An optional url of a type for the concept tree."
                },
                "parts": {
                    "additionalProperties": {
                        "$ref": "#/definitions/ElementTree",
                        "additionalProperties": true
                    },
                    "type": "object",
                    "description": "All parts (element trees) with the main concepts as keys and the subconcepts trees as values."
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Element Tree",
            "description": "An element tree maps elements to poseses (one element can occur in several places) and their relationship between each other (part-whole)."
        },
        "semio.geometry.v1.Point": {
            "properties": {
                "x": {
                    "type": "number",
                    "description": "x coordinate."
                },
                "y": {
                    "type": "number",
                    "description": "y coordinate"
                },
                "z": {
                    "type": "number",
                    "description": "z coordinate"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Point",
            "description": "A 3d point with x,y,z coordinates."
        },
        "semio.model.v1.Pose": {
            "properties": {
                "point_of_view": {
                    "$ref": "#/definitions/semio.geometry.v1.Point",
                    "additionalProperties": true
                },
                "view": {
                    "$ref": "#/definitions/semio.model.v1.Quaternion",
                    "additionalProperties": true
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pose",
            "description": "A pose is a reference frame with a point of view (origin) and orientation (view)."
        },
        "semio.model.v1.Quaternion": {
            "properties": {
                "w": {
                    "type": "number"
                },
                "x": {
                    "type": "number"
                },
                "y": {
                    "type": "number"
                },
                "z": {
                    "type": "number"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Quaternion",
            "description": "Quaternions represent (here) (rotational) orientation. It can be interpreted as the view of an element."
        }
    }
}