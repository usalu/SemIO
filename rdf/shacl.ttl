# baseURI: https://github.com/usalu/semio/releases/download/2/semio.ttl#
# semio Vocabulary
# Version 2
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix sh:   <http://www.w3.org/ns/shacl#> .
@prefix se: <https://github.com/usalu/semio/releases/download/2/semio.ttl#> .
se:
  a owl:Ontology ;
  rdfs:label "semio Vocabulary"@en ;
  rdfs:comment "This vocabulary defines terms used in semio."@en ;
  sh:declare
    [
      sh:prefix "se" ;
      sh:namespace "https://github.com/usalu/semio/releases/download/2/semio.ttl#"^^xsd:anyURI ;
    ] ;
  sh:suggestedShapesGraph <http://www.w3.org/ns/shacl-shacl#> ;
.
se:Archive
  a sh:NodeShape ;
  sh:targetClass se:Archive ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:archiveDependency ;
  sh:name "Archive"@en ;
  sh:description "An archive is a container for a bundle of related artifacts."@en ;
  sh:targetObjectsOf se:archive ;
  sh:property
    [
      sh:path se:archiveDependency ;
      sh:name "archive dependency"@en ;
      sh:description "Another archive that this archive depends on."@en ;
      sh:nodeKind sh:IRI ;
    ] ,
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the archive."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:explanation ;
      sh:name "explanation"@en ;
      sh:description "An explanation of the archive."@en ;
      sh:datatype rdf:langString ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:type ;
    ] ,
    [
      sh:path se:formation ;
    ] ;
.
se:Formation
  a sh:NodeShape ;
  sh:targetClass se:Formation ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:formation ;
  sh:name "Formation"@en ;
  sh:description "A formation is like the assembly manual for a LEGO set."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the formation."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:explanation ;
      sh:name "explanation"@en ;
      sh:description "An explanation of the formation."@en ;
      sh:datatype rdf:langString ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:agent ;
      sh:minCount 1 ;
    ] ,
    [
      sh:path se:attraction ;
    ] ;
.
se:Agent
  a sh:NodeShape ;
  sh:targetClass se:Agent ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:agent ;
  sh:name "Agent"@en ;
  sh:description "An agent is like a LEGO piece inside a set."@en ;
  sh:property
    [
      sh:path se:type ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ;
.
se:Attraction
  a sh:NodeShape ;
  sh:targetClass se:Attraction ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:attraction ;
  sh:name "Attraction"@en ;
  sh:description "An attraction makes the attracted agent being pulled to the attracting agent."@en ;
  sh:property
    [
      sh:path se:attractingSide ;
      sh:class se:Side ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ,
    [
      sh:path se:attractedSide ;
      sh:class se:Side ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ;
.
se:Side
  a sh:NodeShape ;
  sh:targetClass se:Side ; # TODO: create pyshacl issue
  sh:name "Side"@en ;
  sh:description "A side ."@en ;
  sh:property
    [
      sh:path se:agent ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ,
    [
      sh:path se:port ;
      sh:description "The port of the type of the agent of the side."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ;
.
se:Type
  a sh:NodeShape ;
  sh:targetClass se:Type ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:type ;
  sh:name "type"@en ;
  sh:description "A type is like a LEGO piece type. It has ports to connect with other types and it consits of many pieces of information in different formats. In CAD lingo a type is a block (definition)."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the type."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:explanation ;
      sh:name "explanation"@en ;
      sh:description "An explanation of the type."@en ;
      sh:datatype rdf:langString ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:property ;
      sh:name "property"@en ;
      sh:description "A property for the type."@en ;
      sh:or (
        [
          sh:class se:DecimalProperty ;
        ]
        [
          sh:class se:IntegerProperty ;
        ]
        [
          sh:class se:NaturalProperty ;
        ]
        [
          sh:class se:BooleanProperty ;
        ]
        [
          sh:class se:FuzzyProperty ;
        ]
        [
          sh:class se:ChoiceProperty ;
        ]
        [
          sh:class se:BlobProperty ;
        ]
        [
          sh:class se:VectorProperty ;
        ]
        [
          sh:class se:PlaneProperty ;
        ]
      )
    ] ,
    [
      sh:path se:port ;
      sh:name "port"@en ;
      sh:description "A port of the type."@en ;
      sh:minCount 1 ;
    ] ;
.
se:Port
  a sh:NodeShape ;
  sh:targetClass se:Port ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:port ;
  sh:name "port"@en ;
  sh:description "An port is a 3d interface."@en ;
  sh:property
    [
      sh:path se:property ;
    ] ;
.
se:DecimalProperty
  a sh:NodeShape ;
  sh:targetClass se:DecimalProperty ; # TODO: create pyshacl issue
  sh:name "decimal property"@en ;
  sh:description "A decimal property is real number with floating point arithmetic."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the decimal (number) property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:double ; 
    ] ;
.
se:IntegerProperty
  a sh:NodeShape ;
  sh:targetClass se:IntegerProperty ; # TODO: create pyshacl issue
  sh:name "integer property"@en ;
  sh:description "An integer property is a whole number without decimals."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the integer (number) property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:integer ;
    ] ;
.
se:NaturalProperty
  a sh:NodeShape ;
  sh:targetClass se:NaturalProperty ; # TODO: create pyshacl issue
  sh:name "natural property"@en ;
  sh:description "A natural property is positive (=not zero or negative) integer (=no decimals) number."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the natural (number) property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:positiveInteger ; 
    ] ;
.
se:BooleanProperty
  a sh:NodeShape ;
  sh:targetClass se:BooleanProperty ; # TODO: create pyshacl issue
  sh:name "boolean property"@en ;
  sh:description "A boolean property is either true or false."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the boolean property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:boolean ;
    ] ;
.
se:FuzzyProperty
  a sh:NodeShape ;
  sh:targetClass se:FuzzyProperty ; # TODO: create pyshacl issue
  sh:name "fuzzy property"@en ;
  sh:description "A fuzzy (logic) property is a real number between 0 and 1."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the fuzzy (logic) property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:double ;
      sh:minInclusive "0"^^xsd:double ;
      sh:maxInclusive "1"^^xsd:double ;
    ] ;
.
se:ChoiceProperty
  a sh:NodeShape ;
  sh:targetClass se:ChoiceProperty ; # TODO: create pyshacl issue
  sh:name "choice property"@en ;
  sh:description "A choice property is a textual value which represents one choice from a known finite amount of possible choises."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the natural number property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ;
.
se:BlobProperty
  a sh:NodeShape ;
  sh:targetClass se:BlobProperty ; # TODO: create pyshacl issue
  sh:name "blob property"@en ;
  sh:description "A binary large object (blob) property is any data that is stored somewhere else. Most likely it is not rdf."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the natural number property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      se:name "value"@en ;
      se:description "The blob value."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:class se:Blob ;
    ] ;
.
se:Blob
  a sh:NodeShape ;
  sh:targetClass se:Blob ; # TODO: create pyshacl issue
  sh:name "blob"@en ;
  sh:description "A binary large object (blob) is an array of byte with metadata."@en ;
  sh:property
    [
      sh:path se:url ;
      se:name "url"@en ;
      se:description "The url where the blob is stored."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:anyURI ;
    ] ,
    [
      sh:path se:endianness ;
      sh:name "endianness"@en ;
      se:description "The endianness specifcies the order of bytes. Either big-endian (BE) with most siginificant bit first or little-endian (LE) with least significant first."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:in (
        se:BigEndian
        se:LittleEndian
      ) ;
    ] ,
    [
      sh:path se:encoding ;
      sh:name "encoding"@en ;
      sh:description "The encoding format of the blob property."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:in (
        se:Utf8Encoding
        se:Utf16Encoding
        se:ASCIIEncoding
        se:BASE32Encoding
        se:BASE64Encoding
      ) ;
    ] ,
    [
      sh:path se:dataFormat ;
      sh:name "data format"@en ;
      sh:description "The data format of the blob property."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:in (
        se:CustomDataFormat
        se:JsonDataFormat
        se:XmlDataFormat
        se:YamlDataFormat
        se:TomlDataFormat
        se:RdfDataFormat
      ) ;
    ] ,
    [
      sh:path se:schema ;
      sh:name "schema"@en ;
      sh:description "The schema of the blob property."@en ;
      sh:nodeKind sh:IRI ; 
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ;
.

se:PlaneProperty
  a sh:NodeShape ;
  sh:targetClass se:PlaneGeometryProperty ; # TODO: create pyshacl issue
  sh:name "plane property"@en ;
  sh:description "A plane geometry property is ."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the decimal number property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ;
.
