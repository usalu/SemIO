  a sh:NodeShape ;
  sh:targetClass se:PlaneGeometryProperty ; # TODO: create pyshacl issue
  sh:name "plane property"@en ;
  sh:description "A plane is ."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
