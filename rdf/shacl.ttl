# baseURI: https://github.com/usalu/semio/releases/download/2/semio.ttl#
# semio Vocabulary
# Version 2
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix sh:   <http://www.w3.org/ns/shacl#> .
@prefix se: <https://github.com/usalu/semio/releases/download/2/semio.ttl#> .
se:
  a owl:Ontology ;
  rdfs:label "semio Vocabulary"@en ;
  rdfs:comment "This vocabulary defines terms used in semio."@en ;
  sh:declare
    [
      sh:prefix "se" ;
      sh:namespace "https://github.com/usalu/semio/releases/download/2/semio.ttl#"^^xsd:anyURI ;
    ] ;
  sh:suggestedShapesGraph <http://www.w3.org/ns/shacl-shacl#> ;
.
se:Archive
  a sh:NodeShape ;
  sh:targetClass se:Archive ; # TODO: create pyshacl issue
  sh:name "Archive"@en ;
  sh:description "An archive is a container for a bundle of related artifacts."@en ;
  sh:targetObjectsOf se:archive ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the archive."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:explanation ;
      sh:name "explanation"@en ;
      sh:description "An explanation of the archive."@en ;
      sh:datatype rdf:langString ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:type ;
    ] ,
    [
      sh:path se:formation ;
    ] ;
.
se:Formation
  a sh:NodeShape ;
  sh:targetClass se:Formation ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:formation ;
  sh:name "Formation"@en ;
  sh:description "A formation is like the assembly manual for a LEGO set."@en ;
  sh:property
    [
    sh:path se:name ;
    sh:name "name"@en ;
    sh:description "The name of the formation."@en ;
    sh:datatype rdf:langString ;
    sh:minCount 1 ;
    sh:uniqueLang true ;
    ] ,
    [
    sh:path se:explanation ;
    sh:name "explanation"@en ;
    sh:description "An explanation of the formation."@en ;
    sh:datatype rdf:langString ;
    sh:uniqueLang true ;
    ] ,
    [
      sh:path se:agent ;
      sh:minCount 1 ;
    ] ,
    [
      sh:path se:attraction ;
    ] ;
.
se:Agent
  a sh:NodeShape ;
  sh:targetClass se:Agent ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:agent ;
  sh:name "Agent"@en ;
  sh:description "An agent is like a LEGO piece inside a set."@en ;
  sh:property
    [
    sh:path se:name ;
    sh:name "name"@en ;
    sh:description "The name of the agent."@en ;
    sh:datatype rdf:langString ;
    sh:uniqueLang true ;
    ] ,
    [
    sh:path se:explanation ;
    sh:name "explanation"@en ;
    sh:description "An explanation of the agent."@en ;
    sh:datatype rdf:langString ;
    sh:uniqueLang true ;
    ] ,
    [
      sh:path se:type ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ;
.
se:Attraction
  a sh:NodeShape ;
  sh:targetClass se:Attraction ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:attraction ;
  sh:name "Attraction"@en ;
  sh:description "An attraction makes the attracted agent being pulled to the attracting agent."@en ;
  sh:property
    [
      sh:path se:explanation ;
      sh:name "explanation"@en ;
      sh:description "An explanation of the attraction."@en ;
      sh:datatype rdf:langString ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:attractingAttractionPole ;
      sh:class se:AttractionPole ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ,
    [
      sh:path se:attractedAttractionPole ;
      sh:class se:AttractionPole ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ;
.
se:AttractionPole
  a sh:NodeShape ;
  sh:targetClass se:AttractionPole ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:attraction ;
  sh:name "attraction pole"@en ;
  sh:description "An attraction pole describes where an agent connects with another agent."@en ;
  sh:property
    [
      sh:path se:explanation ;
      sh:name "explanation"@en ;
      sh:description "An explanation of the attraction pole."@en ;
      sh:datatype rdf:langString ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:property ;
    ] ;
.
se:Type
  a sh:NodeShape ;
  sh:targetClass se:Type ; # TODO: create pyshacl issue
  sh:targetObjectsOf se:type ;
  sh:name "type"@en ;
  sh:description "A type is like a LEGO piece type. It has ports to connect with other types and it consits of many pieces of information in different formats. In CAD lingo a type is a block (definition)."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the type."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:explanation ;
      sh:name "explanation"@en ;
      sh:description "An explanation of the type."@en ;
      sh:datatype rdf:langString ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:property ;
      sh:name "property"@en ;
      sh:description "A property for the type."@en ;
      sh:or (
        [
          sh:class se:DecimalNumberProperty ;
        ]
        [
          sh:class se:IntegerNumberProperty ;
        ]
        [
          sh:class se:NaturalNumberProperty ;
        ]
        [
          sh:class se:ChoiceProperty ;
        ]
        [
          sh:class se:BlobProperty ;
        ]
      )
    ] ;
.
se:DecimalNumberProperty
  a sh:NodeShape ;
  sh:targetClass se:DecimalNumberProperty ; # TODO: create pyshacl issue
  sh:name "decimal number property"@en ;
  sh:description "A decimal number property is real number with floating point arithmetic."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the decimal number property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:double ; 
    ] ;
.
se:IntegerNumberProperty
  a sh:NodeShape ;
  sh:targetClass se:IntegerNumberProperty ; # TODO: create pyshacl issue
  sh:name "integer number property"@en ;
  sh:description "An integer number (whole number) property."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the integer number property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:integer ;
    ] ;
.
se:NaturalNumberProperty
  a sh:NodeShape ;
  sh:targetClass se:NaturalNumberProperty ; # TODO: create pyshacl issue
  sh:name "natural number property"@en ;
  sh:description "A natural number property is positive (=not zero or negative) integer (=no decimals) number."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the natural number property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:positiveInteger ; 
    ] ;
.
se:ChoiceProperty
  a sh:NodeShape ;
  sh:targetClass se:ChoiceProperty ; # TODO: create pyshacl issue
  sh:name "choice property"@en ;
  sh:description "A choice property is a textual value which represents one choice from a known finite amount of possible choises."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the natural number property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ;
.
se:TrueOrFalseProperty
  a sh:NodeShape ;
  sh:targetClass se:TrueOrFalseProperty ; # TODO: create pyshacl issue
  sh:name "true or false property"@en ;
  sh:description "A true of false property is either true or false."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the true or false property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:datatype xsd:boolean ;
    ] ;
.
se:BlobProperty
  a sh:NodeShape ;
  sh:targetClass se:BlobProperty ; # TODO: create pyshacl issue
  sh:name "blob property"@en ;
  sh:description "A binary large object (blob) property is any data that is stored somewhere else. Most likely it is not rdf."@en ;
  sh:property
    [
      sh:path se:name ;
      sh:name "name"@en ;
      sh:description "The name of the natural number property."@en ;
      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:value ;
      se:name "value"@en ;
      se:description "The url value where the blob is stored."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:nodeKind sh:IRI ;
    ] ,
    [
      sh:path se:endianness ;
      sh:name "endianness"@en ;
      se:description "The endianness specifcies the order of bytes. Either big-endian (BE) with most siginificant bit first or little-endian (LE) with least significant first."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:in (
        se:BigEndian
        se:LittleEndian
      ) ;
    ] ,
    [
      sh:path se:encoding ;
      sh:name "encoding"@en ;
      sh:description "The encoding format of the blob property."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:in (
        se:Utf8Encoding
        se:Utf16Encoding
        se:ASCIIEncoding
        se:BASE32Encoding
        se:BASE64Encoding
      ) ;
    ] ,
    [
      sh:path se:dataFormat ;
      sh:name "data format"@en ;
      sh:description "The data format of the blob property."@en ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:in (
        se:CustomDataFormat
        se:JsonDataFormat
        se:XmlDataFormat
        se:YamlDataFormat
        se:TomlDataFormat
        se:RdfDataFormat
      ) ;
    ] ,
    [
      sh:path se:schema ;
      sh:name "schema"@en ;
      sh:description "The schema of the blob property."@en ;
      sh:nodeKind sh:IRI ; 
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] ;
.