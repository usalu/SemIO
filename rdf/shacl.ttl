      sh:datatype rdf:langString ;
      sh:minCount 1 ;
      sh:uniqueLang true ;
    ] ,
    [
      sh:path se:origin ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:class se:Point ; 
    ] ,
    [
      sh:path se:xAxis ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:class se:Vector ; 
    ] ,
    [
      sh:path se:yAxis ;
