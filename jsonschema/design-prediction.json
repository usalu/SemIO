                "id_": {
                    "default": "",
                    "description": "\ud83c\udd94 The id of the port.",
                    "maxLength": 128,
                    "title": "Id ",
                    "type": "string"
                }
            },
            "title": "PortId",
            "type": "object"
        },
        "Side": {
