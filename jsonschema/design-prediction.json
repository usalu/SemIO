                "id_": {
                    "default": "",
                    "description": "\ud83c\udd94 The id of the piece.",
                    "maxLength": 128,
                    "title": "Id ",
                    "type": "string"
                },
                "type": {
                    "$ref": "#/$defs/TypeId",
                    "description": "\ud83e\udde9 The type of the piece."
                }
            },
            "required": [
                "type"
